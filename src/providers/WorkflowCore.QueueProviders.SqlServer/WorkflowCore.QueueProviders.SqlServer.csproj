    <Authors>Roberto Paterlini</Authors>
    <Description>Queue provider for Workflow-core using SQL Server Service Broker</Description>
    <Company />
    <Stage>alpha</Stage>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Services\DequeueWork.sql" />
    <None Remove="Services\QueueWork.sql" />
